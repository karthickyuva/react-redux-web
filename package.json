{
  "name": "react-redux-web-app",
  "version": "1.0.0",
  "description": "Frontend JS web app",
  "engines": {
    "node": ">=6.2.0"
  },
  "scripts": {
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "test": "npm run lint && jest",
    "clean": "rimraf dist",
    "start": "node server.js",
    "csslint": "echo CSSLint: && ./node_modules/.bin/csslint assets/css/*",
    "lint": "echo ESLint: && ./node_modules/.bin/eslint -v && ./node_modules/.bin/eslint . --ignore-path .gitignore && npm run csslint",
    "setup": "echo \"//registry.npmjs.org/:_authToken=${NPM_TOKEN}\" >> .npmrc"
  },
  "author": "Karthick",
  "dependencies": {
